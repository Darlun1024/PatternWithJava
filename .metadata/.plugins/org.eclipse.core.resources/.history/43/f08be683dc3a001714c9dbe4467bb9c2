package com.wsj.pattern.state;

public class Teenager extends State {

	public Teenager(Person person) {
		super(person);
		// TODO Auto-generated constructor stub
	}

	@Override
	void run100() {
		if(person.age > 16){
			person.setState(new Teenager(person));
			person.run100();
		}else{
			System.out.println("摔了四五次，4分钟跑完全程");
		}
	}

}
