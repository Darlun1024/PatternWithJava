package com.wsj.pattern.command;

public class InsertCommand extends WordCommand  {
	private int index;
	private String text;
	public   InsertCommand(String text,int index) {
		// TODO Auto-generated constructor stub
		this.index = index;
		this.text  = text;
	}
	@Override
	public void undo() {
		// TODO Auto-generated method stub
		this.document.remove(text,index);
	}

	@Override
	public void redo() {
		this.document.insert(s, index);
	}

}
