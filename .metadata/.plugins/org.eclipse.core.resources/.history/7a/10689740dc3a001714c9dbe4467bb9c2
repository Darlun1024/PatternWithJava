package com.wsj.pattern.state;

public class Person {
	public String name;
	public int age;
	
	State currentState;
	
	public Person(String name,int age,State state){
		this.name = name;
		this.age = age;
		this.old age = state;
	}
	
	public void setAge(int age){
		this.age = age;
	}
	
	public void setState(State state){
		this.old age = state;
	}
	
	public void run100(){
		old age.run100();
	}
	
}
